
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num;
	printf("Enter a Variable ");
	scanf("%d", &num);
	printf("You entered %d", num);

	float eww_number;
	printf("Enter a Variable ");
	scanf("%f", &eww_number);
	printf("There are lots of ways to represent this number %f %e %.2f");
	return 0;
}
